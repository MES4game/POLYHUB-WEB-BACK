{
    "name": "et4-project-web-back",
    "version": "1.0.0",
    "description": "Back-end of project \"PolyHUB\" for ET4 web project",
    "private": true,
    "main": "build/index.js",
    "type": "commonjs",
    "scripts": {
        "lint": "node scripts/lint.mjs",
        "dev": "node scripts/dev.mjs",
        "build": "node scripts/build.mjs",
        "start": "node scripts/start.mjs",
        "lint:run": "eslint 'src/**/*.[jt]s' --max-warnings=0 --cache",
        "nibble:run": "eslint-nibble 'src/**/*.[jt]s' --cache",
        "dev:run": "NODE_ENV='development' tsnd --respawn -r tsconfig-paths/register src/index.ts",
        "build:run": "tsc && tsc-alias",
        "start:run": "NODE_ENV='production' node build/index.js"
    },
    "keywords": [
        "typescript",
        "eslint",
        "express",
        "swagger",
        "mysql"
    ],
    "author": "Maxime DAUPHIN, MaÃ«l HOUPLINE, Julien TAP",
    "license": "MIT",
    "dependencies": {
        "compression": "1.8.1",
        "cors": "2.8.5",
        "express": "5.1.0",
        "helmet": "8.1.0",
        "morgan": "1.10.1",
        "mysql2": "3.15.2",
        "swagger-jsdoc": "6.2.8",
        "swagger-ui-express": "5.0.1"
    },
    "devDependencies": {
        "@eslint/js": "9.38.0",
        "@stylistic/eslint-plugin": "5.5.0",
        "@types/compression": "1.8.1",
        "@types/cors": "2.8.19",
        "@types/express": "5.0.3",
        "@types/morgan": "1.9.10",
        "@types/node": "^24.6.2",
        "@types/swagger-jsdoc": "6.0.4",
        "@types/swagger-ui-express": "4.1.8",
        "@typescript-eslint/eslint-plugin": "8.46.1",
        "@typescript-eslint/parser": "8.46.1",
        "eslint": "9.38.0",
        "eslint-nibble": "9.1.0",
        "node": "^24.9.0",
        "ts-node": "10.9.2",
        "ts-node-dev": "2.0.0",
        "tsc-alias": "1.8.16",
        "tsconfig-paths": "4.2.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.46.1"
    }
}
